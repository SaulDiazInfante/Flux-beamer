@Article{Appleby2006,
  author    = {Appleby, J. and Mackey, D. and Rodkina, A.},
  title     = {Almost sure polynomial asymptotic stability of stochastic difference equations},
  journal   = {Journal of Mathematical Sciences},
  year      = {2006},
