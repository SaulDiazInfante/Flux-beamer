    during bone rem.pdf:pdf},
  keywords = {Bone remodeling,External agents,Nonlinear stability analysis,Periodic solutions},
  pmid     = {25784536},
  url      = {http://www.sciencedirect.com/science/article/pii/S0025556415000620},
}

